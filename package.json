{
  "name": "@salesforce/change-case-management",
  "version": "1.1.0",
  "bugs": "https://github.com/forcedotcom/change-case-management/issues",
  "bin": {
    "sfchangecase": "./bin/run"
  },
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/errors": "^1",
    "@salesforce/command": "^2",
    "@salesforce/core": "^2.4.0",
    "@salesforce/kit": "^1.2.2",
    "@salesforce/ts-types": "^1.2.2",
    "shelljs": "^0.8.3",
    "ts-node": "^8",
    "tslib": "^1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/plugin-help": "^2",
    "@oclif/test": "^1",
    "@salesforce/dev-config": "1.4.1",
    "@types/chai": "^4",
    "@types/mocha": "^5",
    "@types/node": "^10",
    "chai": "^4",
    "globby": "^8",
    "mocha": "^5",
    "nyc": "^14",
    "tslint": "^5"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/lib",
    "/messages",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/forcedotcom/change-case-management",
  "keywords": [
    "sfdx-plugin"
  ],
  "license": "ISC",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "sfchangecase",
    "topics": {
      "hello": {
        "description": "Commands to say hello."
      }
    },
    "devPlugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "forcedotcom/change-case-management",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "tslint -p test -t stylish",
    "clean": "rm -rf lib",
    "build": "tsc -b",
    "prepack": "yarn clean && yarn build && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "publishConfig": {
    "access": "public"
  }
}
