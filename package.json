{
  "name": "@salesforce/change-case-management",
  "version": "1.5.0",
  "bugs": "https://github.com/forcedotcom/change-case-management/issues",
  "bin": {
    "sfchangecase": "./bin/run"
  },
  "dependencies": {
    "@oclif/config": "^1.18.3",
    "@salesforce/command": "^3.1.0",
    "@salesforce/core": "^2.20.0",
    "@salesforce/kit": "^1.5.41",
    "@salesforce/ts-types": "^1.2.2",
    "shelljs": "^0.8.3",
    "tslib": "^2"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/plugin-command-snapshot": "^2.2.2",
    "@salesforce/dev-config": "^3.0.1",
    "@salesforce/dev-scripts": "^2.0.0",
    "@salesforce/plugin-command-reference": "^1.3.0",
    "@salesforce/prettier-config": "^0.0.2",
    "@salesforce/ts-sinon": "^1.3.21",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "chai": "^4.3.6",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.27.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-salesforce": "^0.1.6",
    "eslint-config-salesforce-license": "^0.1.6",
    "eslint-config-salesforce-typescript": "^0.2.7",
    "eslint-plugin-header": "^3.0.0",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-jsdoc": "^35.1.2",
    "eslint-plugin-prettier": "^3.4.1",
    "globby": "^8",
    "husky": "^7.0.4",
    "lint-staged": "^10.5.4",
    "mocha": "^9.2.1",
    "nyc": "^15.1.0",
    "prettier": "^2.0.5",
    "pretty-quick": "^3.1.0",
    "shx": "^0.3.4",
    "sinon": "10.0.0",
    "ts-node": "^10.0.0",
    "typescript": "^4.1.3"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "files": [
    "/lib",
    "/messages",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/forcedotcom/change-case-management",
  "keywords": [
    "sfdx-plugin"
  ],
  "license": "BSD-3-Clause",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "sfchangecase",
    "devPlugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-command-snapshot",
      "@salesforce/plugin-command-reference"
    ]
  },
  "repository": "forcedotcom/change-case-management",
  "scripts": {
    "build": "sf-build",
    "clean": "sf-clean",
    "clean-all": "sf-clean all",
    "clean:lib": "shx rm -rf lib && shx rm -rf coverage && shx rm -rf .nyc_output && shx rm -f oclif.manifest.json",
    "compile": "sf-compile",
    "docs": "sf-docs",
    "format": "sf-format",
    "lint": "sf-lint",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "yarn lint && yarn test:deprecation-policy && yarn test:command-reference",
    "prepack": "sf-prepack",
    "prepare": "sf-install",
    "pretest": "sf-compile-test",
    "test": "sf-test",
    "test:command-reference": "./bin/run commandreference:generate --erroronwarnings",
    "test:deprecation-policy": "./bin/run snapshot:compare",
    "test:nuts": "nyc mocha \"**/*.nut.ts\" --slow 4500 --timeout 600000 --parallel",
    "version": "oclif-dev readme"
  },
  "publishConfig": {
    "access": "public"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
